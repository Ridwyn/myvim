{
	"suggest.noselect": true,
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
    "dockercompose": {
      "command": "docker-compose-langserver",
      "args": ["--stdio"],
      "filetypes": ["dockercompose"],
      "rootPatterns": [".git", ".env", "docker-compose.yml", "compose.yml"]
    }
  },
  "git.enableGutters": false,
  "java.imports.gradle.wrapper.checksums": [
    {
      "sha256": "df0124be72626f5aef1badfd39039b976e91ffcf4409faca5344053b441fb939",
      "allowed": true
    }
  ]
}
